{
  "root": true,
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "extends": ["next", "next/core-web-vitals", "eslint:recommended"],
  "rules": {
    "no-unused-vars": "off",
    "no-undef": "off",
    "no-case-declarations": "off"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
